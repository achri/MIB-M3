body{
margin-top: 5px;
padding:0;
text-align: center;
font-family: verdana, arial, sans-serif;
font-size: 10pt;
background: #dddddd;
}
 
/* Layout */

#pagewidth{
width:938px;
text-align:left;
margin-left:auto;
margin-right:auto;
margin-bottom: 2px;
background: #ffffff;
padding: 0px;
min-height: 600px;
overflow: visible;
}

#main_head{
	width: 100%;
 	height: 80px;
 	background: url(../img/head.jpg) no-repeat;
}

#content{
width: 99%;
overflow: visible;
margin-top: 5px;
margin-left: 5px;
text-align:left;
color: #000000;
padding: 0px;
}
